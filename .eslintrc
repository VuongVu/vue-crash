{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["plugin:vue/vue3-recommended", "prettier"],
    "plugins": ["vue", "prettier"],
    "rules": {
        "prettier/prettier": "error"
    }
}
